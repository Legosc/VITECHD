@{
    ViewBag.Title = "Acerca de Mi";
}
<section class="container">
    <div class="row">
        <div class="col col-md-6">
        <div class="card">
            <div class="card-body">
                <h3>Habilidades de nuestro equipo</h3>
                <div class="row">
                    <div class="col col-12">
                        <p>HTML</p>
                        <div class="progress z-depth-1">
                            <div class="progress-bar z-depth-3" role="progressbar" style="width: 90%; background-color: #0aa4d2;" aria-valuenow="90" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>
                        <p>Javascript</p>
                        <div class="progress z-depth-1">
                            <div class="progress-bar z-depth-3" role="progressbar" style="width: 85%; background-color: #0aa4d2;" aria-valuenow="85" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>
                        <p>SQL</p>
                        <div class="progress z-depth-1">
                            <div class="progress-bar z-depth-3" role="progressbar" style="width: 80%; background-color: #0aa4d2;" aria-valuenow="80" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>
                        <p>ORACLE</p>
                        <div class="progress z-depth-1">
                            <div class="progress-bar z-depth-3" role="progressbar" style="width: 85%; background-color: #0aa4d2;" aria-valuenow="95" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>
                        <p>Asp.net - C#</p>
                        <div class="progress z-depth-1">
                            <div class="progress-bar z-depth-3" role="progressbar" style="width: 90%; background-color: #0aa4d2;" aria-valuenow="90" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>
                        <p>Asp.net - Vb</p>
                        <div class="progress z-depth-1">
                            <div class="progress-bar z-depth-3" role="progressbar" style="width: 70%; background-color: #0aa4d2;" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>
                        <p>Angular</p>
                        <div class="progress z-depth-1">
                            <div class="progress-bar z-depth-3" role="progressbar" style="width: 40%; background-color: #0aa4d2;" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>
                        <p>NodeJs</p>
                        <div class="progress z-depth-1">
                            <div class="progress-bar z-depth-3" role="progressbar" style="width: 45%; background-color: #0aa4d2;" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>
                    </div>
                </div>
                </div>
            </div>
        </div>
        <div class="col col-md-6">
            <div class="card">
                <div class="card-body">
                    <h3>Experiencias</h3>
                    <div class="row">
                        <div class="col-12">
                            <h4>Total de Proyectos</h4>
                            <span class="alert-success">5</span>
                        </div>
                        <div class="col-md-6">
                            <h4>Actuales</h4>
                            <span class="alert-success">2</span>
                        </div>
                        <div class="col-md-6">
                            <h4>Exitosos</h4>
                            <span class="alert-success">3</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col col-md-12">
            <div class="card">
                <div class="card-body">
                    <h3>Nuestros Clientes</h3>
                    <div class="row">

                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}